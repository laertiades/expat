<div class="col-md-6">
    <%= f.label :name, "Account Name" %>
    <%= f.text_field :name, :class => "form-control" %>
    <div class="clear"></div>
    
    <%= f.label :manager_id, "Manager" %>
    <%= f.collection_select(:manager_id, Manager.all, :id, :name) %>
    <div class="clear"></div>

    <%= f.label :consultant_id, "Consultant" %>
    <%= f.collection_select(:consultant_id, Consultant.all, :id, :name) %>
    <div class="clear"></div>
    
    <%= f.label :received, "Quest/Note Received" %>
    <%= f.date_field :received, :class => "form-control" %>
    <div class="clear"></div>
    
    <%= f.label :assigned, "Return Assigned" %>
    <%= f.date_field :assigned, :class => "form-control" %>
    <div class="clear"></div>
    
    <%= f.label :began, "Began processing return" %>
    <%= f.date_field :began, :class => "form-control" %>
    <div class="clear"></div>
    
    <%= f.label :completed, "Return Completed" %>
    <%= f.date_field :completed, :class => "form-control" %>
    <div class="clear"></div>
    
    <%= f.label :sent, "Sent to Client" %>
    <%= f.date_field :sent, :class => "form-control" %>
    <div class="clear"></div>
    
    <%= f.label :filed, "E Filed" %>
    <%= f.date_field :filed, :class => "form-control" %>
    <div class="clear"></div>

    <%= f.label :status, "Account Status" %>
    <%= f.select(:status, options_for_select([['Normal', 1], ['Awaiting Data',2], ['Client Not Responding',3], ['Client Quit',4]], @account.status)) %>
</div>
